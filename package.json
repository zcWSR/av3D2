    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-qunar": "^2.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "^3.0.6",
    "stats.js": "^0.17.0",
    "webpack": "3.1.0",
    "webpack-dev-server": "2.11.0"
  }
}
