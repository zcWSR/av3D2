{
  "name": "av3d2",
  "version": "1.0.0",
  "description": "a simple web 3d audio visualizer base on three.js",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --devtool eval-source-map --progress --colors --hot --inline --content-base ./dev",
    "build": "rm -rf build && webpack -p --progress --profile --colors --config webpack.prod.config.js"
  },
  "keywords": [
    "three.js",
    "es6",
    "canvas3D"
  ],
  "author": "zcWSR",
  "license": "MIT",
